{
    "name": "Pocket ID",
    "version": "0.26",
    "slug": "pocket_id",
    "description": "Simple OIDC provider for passkey authentication",
    "arch": [
        "amd64",
        "aarch64"
    ],
    "startup": "application",
    "boot": "auto",
    "options": {
        "PUBLIC_APP_URL": "http://localhost",
        "TRUST_PROXY": false,
        "MAXMIND_LICENSE_KEY": ""
    },
    "schema": {
        "PUBLIC_APP_URL": "str",
        "TRUST_PROXY": "bool",
        "MAXMIND_LICENSE_KEY": "str?"
    },
    "ports": {
        "80/tcp": 7080
    },
    "map": [
        {
            "type": "data",
            "path": "/app/backend/data"
        }
    ],
    "init": false
}